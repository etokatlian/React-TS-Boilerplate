{
  "name": "cf-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --inline --hot",
    "test": "jest",
    "tslint": "tslint './src/**/*.{ts,tsx}'",
    "tslint:fix": "npm run tslint -- --fix",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier --write --config ./prettierrc.json './src/**/*.{ts,tsx}'",
    "dev": "npm run clean && webpack --config webpack.config.js",
    "build": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "test": "jest",
    "clean": "rimraf dist",
    "serve": "node server.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "jest": "23.6.0",
    "prettier": "^1.16.1",
    "react-hot-loader": "^4.6.3",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3"
  }
}