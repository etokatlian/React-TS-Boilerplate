{
  "name": "cf-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js",
    "test": "jest",
    "tslint": "tslint --config tslint.json './src/**/*.{ts,tsx}'",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier --config prettierrc.json --write \"{,!(node_modules|dist|build|coverage|config)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "yarn prettier && yarn tslint",
    "dev": "yarn run clean && webpack --config ./config/webpack.config.js",
    "build": "yarn run clean && webpack --config ./config/webpack.config.prod.js --progress",
    "clean": "rimraf dist",
    "serve": "node server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit",
      "pre-push": "yarn pre-commit"
    }
  },
  "dependencies": {
